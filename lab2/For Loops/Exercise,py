#1
for x in range(3):
  print(x)
#0 1 2

#2
fruits = ["apple", "banana", "cherry"]
for x in fruits:
  print(x)
#for and in

#3
fruits = ["apple", "banana", "cherry"]
for x in fruits:
  if x == "banana":
    continue
  print(x)
#continue

#4
for x in range(6):
  print(x)
#range(6)

#5
fruits = ["apple", "banana", "cherry"]
for x in fruits:
  if x == "banana":
    break
  print(x)
#break